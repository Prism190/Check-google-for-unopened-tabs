{
  "manifest_version": 3,
  "name": "Tab Checker",
  "version": "1.0",
  "description": "Compares currently open tabs with a built-in list of recovered session URLs.",
  "background": {
  "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "windows"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
